<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.2.xsd"
        default-init-method="defaultInit" default-destroy-method="defaultDestroy">
        
	<bean name="user" class="com.adequate.beans.User" scope="prototype">
	</bean>
	
	<bean name="userInfo" class="com.adequate.beans.UserInfo" scope="prototype">
	</bean>
	
	<bean name="review" class="com.adequate.beans.Review" scope="prototype">
	</bean>
	
	<bean name="itinerary" class="com.adequate.beans.Itinerary" scope="prototype">
	</bean>
	
	<bean name="locationList" class="com.adequate.beans.LocationList" scope="prototype">
	</bean>

</beans>